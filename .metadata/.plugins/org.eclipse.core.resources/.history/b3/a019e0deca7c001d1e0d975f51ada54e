package lista_de_exercicio;

import java.util.Locale;
import java.util.Scanner;

public class Ex009 {

	public static void main(String[] args) {
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		
		double salario = 1.212;
		System.out.println("Informe seu salario: ");
		double salarioUser = sc.nextDouble();
		
		double diferencialSalario = salario / salarioUser;
		System.out.printf("A quantidade de salarios em relação ao salario minimo é de: ",diferencialSalario);
		sc.close();
	}

}
