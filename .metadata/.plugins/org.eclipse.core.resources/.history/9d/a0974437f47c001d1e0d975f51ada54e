		int valor;
		int crescente;
		System.out.println("Do maior para o menor");
		
		System.out.println("Insira o valor de A: ");
		int A = sc.nextInt();
		System.out.println("Insira o valor de B: ");
		int B = sc.nextInt();
		System.out.println("Insira o valor de C: ");
		int C = sc.nextInt();
		
		
		
		sc.close();

	}

}
